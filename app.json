{
    "expo": {
        "name": "Trackademix",
        "slug": "trackademix",
        "scheme": "trackademix",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/icon.png",
        "splash": {
            "image": "./assets/splash-icon.png",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
        },
        "assetBundlePatterns": ["**/*"],
        "ios": { "supportsTablet": true },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/adaptive-icon.png",
                "backgroundColor": "#ffffff"
            }
        },
        "web": { "bundler": "metro", "favicon": "./assets/favicon.png" }
    }
}
